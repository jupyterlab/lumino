{
  "name": "@lumino/application",
  "version": "1.22.0",
  "description": "Lumino Pluggable Application",
  "homepage": "https://github.com/jupyterlab/lumino",
  "bugs": {
    "url": "https://github.com/jupyterlab/lumino/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/lumino.git"
  },
  "license": "BSD-3-Clause",
  "author": "S. Chris Colbert <sccolbert@gmail.com>",
  "contributors": [
    "A. Darian <git@darian.af>",
    "Dave Willmer <dave.willmer@gmail.com>",
    "S. Chris Colbert <sccolbert@gmail.com>",
    "Steven Silvester <steven.silvester@gmail.com>"
  ],
  "files": [
    "dist/*",
    "src/*",
    "types/*"
  ],
  "main": "dist/index.js",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.es6",
  "types": "types/index.d.ts",
  "scripts": {
    "api": "api-extractor run --local --verbose",
    "build": "npm run build:src && rollup -c",
    "build:src": "tsc --build",
    "build:test": "tsc --build tests && cd tests && webpack",
    "clean": "rimraf ./lib && rimraf *.tsbuildinfo && rimraf ./types && rimraf ./dist",
    "clean:test": "rimraf tests/build",
    "docs": "typedoc --options tdoptions.json src",
    "minimize": "terser dist/index.js -c -m --source-map \"content='dist/index.js.map',url='index.min.js.map'\" -o dist/index.min.js",
    "test": "npm run test:firefox",
    "test:chrome": "cd tests && karma start --browsers=Chrome",
    "test:firefox": "cd tests && karma start --browsers=Firefox",
    "test:ie": "cd tests && karma start --browsers=IE",
    "watch": "tsc --build --watch"
  },
  "dependencies": {
    "@lumino/commands": "^1.15.0",
    "@lumino/coreutils": "^1.8.0",
    "@lumino/widgets": "^1.25.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.6.0",
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.39",
    "chai": "^3.5.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "mocha": "^6.2.2",
    "rimraf": "^2.5.2",
    "rollup": "^2.22.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "terser": "^4.6.2",
    "tslib": "1.10.0",
    "typedoc": "~0.15.0",
    "typescript": "~3.6.0",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10"
  },
  "publishConfig": {
    "access": "public"
  },
  "jsdelivr": "dist/index.min.js"
}
